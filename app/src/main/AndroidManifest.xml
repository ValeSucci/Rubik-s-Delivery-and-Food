<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="notanamelessentreprise.rubiksdeliveryandfood">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MenuPrincipalActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".MenuHamburguesaActivity" />
        <activity android:name=".MenuPizzaActivity" />
        <activity android:name=".MenuHotDogActivity" />
        <activity android:name=".MenuPolloActivity" />
        <activity android:name=".MenuExtrasActivity" />
        <activity android:name=".VistaDeDetalleActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="Login" />
        <activity android:name=".ContactenosActivity" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".CalculadoraActivity" />
        <activity android:name=".ArmaloHamburguesaActivity" />
        <activity android:name=".ArmaloPizzaActivity" />
        <activity android:name=".ArmaloHotDogActivity" />
        <activity
            android:name=".RegistroActivity"
            android:label="Registro de Usuario" />
        <activity android:name=".VerOrdenActivity" />
        <activity android:name=".VideoActivity" />
        <activity android:name=".GraciasActivity"></activity>
    </application>

</manifest>